---
layout: kml
title: Kml file for map icons
---

<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
<Document>
    <Style id="redDot">
      <IconStyle>
        <Icon>
          <href>http://labs.google.com/ridefinder/images/mm_20_red.png
          </href>
        </Icon>
      </IconStyle>
    </Style>

    <Style id="blueDot">
      <IconStyle>
        <Icon>
          <href>http://labs.google.com/ridefinder/images/mm_20_blue.png
          </href>
        </Icon>
      </IconStyle>
    </Style>


    {% for row in site.data.allGumLocsNoCounty2013.content_hash %}
     <Placemark>
    <name>{{row.display_name}}</name>
    <styleUrl>#redDot</styleUrl>
    <description>
      {{row.radius}}
    </description>
    <Point>
      <coordinates>{{row.centroid_long}},{{row.centroid_lat}},5</coordinates>
    </Point>
  </Placemark>
    {% endfor %}

    {% for row in site.data.allGumLocsNoCounty.content_hash %}
     <Placemark>
    <name>{{row.display_name}}</name>
    <styleUrl>#blueDot</styleUrl>
    <description>
      {{row.radius}}
    </description>
    <Point>
      <coordinates>{{row.centroid_long}},{{row.centroid_lat}},0</coordinates>
    </Point>
  </Placemark>
    {% endfor %}
 
  </Document>
</kml>

